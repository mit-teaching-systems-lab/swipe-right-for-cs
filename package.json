{
  "name": "swipe-right-for-cs",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "install": "cd client && YARN_PRODUCTION=false yarn install --frozen-lockfile",
    "start": "yarn run dev",
    "test": "yarn run lint && yarn run jest && cd client && yarn run test",
    "heroku-postbuild": "cd client && yarn run build",
    "dev": "concurrently \"yarn run server-dev\" \"yarn run client\" \"yarn run storybook\"",
    "server": "node server/index.js",
    "server-dev": "NODE_ENV=development DATABASE_URL=http://localhost:5432/swipe-right-db yarn run server",
    "client": "cd client && yarn run start",
    "storybook": "cd client && yarn run storybook",
    "lint": "eslint --ext js -c server/.eslintrc server scripts",
    "jest": "jest server",
    "ngrok": "ngrok http 3000 -host-header=\"localhost:8080\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "express-rate-limit": "^2.9.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0",
    "node-fetch": "^1.7.3",
    "pg": "^7.3.0",
    "react": "16",
    "superagent": "^3.6.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "eslint": "^4.8.0",
    "eslint-plugin-node": "^5.2.0",
    "ngrok": "^2.2.22"
  }
}
